{
  "version": 2,
  "framework": "vite",
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "installCommand": "npm install",
  "devCommand": "npm run dev",
  "rewrites": [
    {
      "source": "/api/health",
      "destination": "/api/health"
    },
    {
      "source": "/api/auth",
      "destination": "/api/auth"
    },
    {
      "source": "/api/applications",
      "destination": "/api/applications"
    },
    {
      "source": "/api/contact",
      "destination": "/api/contact"
    },
    {
      "source": "/api/gallery",
      "destination": "/api/gallery"
    },
    {
      "source": "/api/newsletter",
      "destination": "/api/newsletter"
    },
    {
      "source": "/api/settings",
      "destination": "/api/settings"
    },
    {
      "source": "/api/voice",
      "destination": "/api/voice"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}
